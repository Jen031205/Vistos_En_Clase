Proceso Divisas
	Definir cantidad, mitad como real;
	Definir euro, dolar como real;
	
	Escribir "Ingresa la cantidad de Pesos";
	Leer cantidad;
	
	mitad <-cantidad/2;
	dolar<- mitad/17.85;
	
	Escribir "La cantidad de dolares es ", dolar;
	
	euro<- mitad/18.93;
	Escribir "La cantidad de euro es £", euro;
	
	
FinProceso
