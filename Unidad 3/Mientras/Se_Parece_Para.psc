Proceso Se_Parece_Para
	Definir contador  Como Entero;
	
	Para contador<-1 Hasta 10 Con Paso 1 Hacer
		Escribir "Hola ";
	FinPara
	
	contador<-1; //Inicializacion
	Mientras contador <= 10 Hacer //Condicion
		Escribir contador, "Hola ";
		contador<-contador+1;  //Incremento o paso
	FinMientras
	
	
FinProceso
